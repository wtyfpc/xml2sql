                   display_name="CPU利用率"
                   description="Linux服务区CPU利用率"
                   data_type="1"
                   unit="%"
                   part_type="cpu"
                   interval="90"
                   iscollect="1"
                   pre_process=""
                   post_process=""
                   collect_mode="1"
                   batch_group="group1"
                   priority="0"
                   protocol="http"
                   protocol_param="/linux/monitor/main"
                   value_keyword="linux_cpu_usage"
                   value_range="0~100"
                   value_mapping="[{'data': '1','zh': '可用','en': 'Available'},{'data': '2','zh': '不可用','en': 'Unavailable'}]"
                   create_version="6.5"
                   last_modify_version="6.6"
